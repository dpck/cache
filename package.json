{
  "name": "@depack/cache",
  "version": "1.1.0",
  "description": "Computes Necessary Information To Cache A Module, And Allows To Check If It Has Been Updated.",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-compile": "yarn test -e test-compile",
    "lib": "depack-dev src/depack -o compile/cache.js -a -c -p -s --source_map_include_content",
    "lint": "eslint .",
    "doc": "doc -o README.md -d -a -n _depack",
    "d": "yarn-s d1 externs",
    "d1": "typal compile/index.js -c",
    "externs": "typal types/externs.js",
    "e": "alanode"
  },
  "files": [
    "compile",
    "types/externs.js",
    "typedefs.json",
    "src"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/dpck/cache.git"
  },
  "typedefs": "typedefs.json",
  "keywords": [
    "cache",
    "depack",
    "modules",
    "mtime",
    "dependencies",
    "update"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dpck/cache/issues"
  },
  "homepage": "https://github.com/dpck/cache#readme",
  "devDependencies": {
    "@depack/detect": "^1.1.0",
    "@depack/split": "^1.0.0",
    "alamode": "^3.5.2",
    "documentary": "^1.37.0",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "^1.0.1",
    "fpj": "^1.3.1",
    "resolve-dependency": "^1.0.3",
    "static-analysis": "^2.1.1",
    "temp-context": "^2.1.3",
    "yarn-s": "^1.2.3",
    "zoroaster": "^4.3.0"
  },
  "srcDependencies": {
    "erte": "^1.2.0",
    "makepromise": "^3.1.0",
    "resolve-dependency": "^1.0.3",
    "static-analysis": "^1.7.1"
  }
}
